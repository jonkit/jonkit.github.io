<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
<channel>
<title>The Only Site Ever Briefly</title>
<link>http://theonlysiteever.com/</link>
<description>Brief snippets of text posted to this sites</description>

 {% for post in site.categories.briefly %}
<item>
<link>{{ site.url }}{{ site.baseurl }}{{ post.url }}</link>
<guid>{{ site.url }}{{ site.baseurl }}{{ post.url }}</guid>
 <content:encoded><![CDATA[{{ post.content }}]]></content>
 <description>Brief text snippet</description>
</item>
 {% endfor %}
 </channel>
 </rss>
